style computer_text is default:
    color "#000"
    outlines []
    align (0.0, 0.0)
    layout "tex"
    size 22

style computer_button:
    background Solid("#afafaf")
    selected_background Solid("#1180ff")
    insensitive_background Solid("#bcd7ff")
    xysize (165, 40)

style computer_button_text:
    size 24
    color "#fff"
    align (0.5, 0.5)
    text_align 0.5
    font "mod_assets/STUFF/RobotoMedium.ttf"
    outlines []

style computer_slider is slider:
    ymargin 2

style computer_vbox:
    spacing 5
